{
    "name": "yiisoft/router-fastroute",
    "type": "library",
    "description": "Yii router FastRoute adpapter",
    "keywords": [
        "router",
        "fastroute"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/router-fastroute/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/router-fastroute"
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "nikic/fast-route": "^1.3",
        "psr/simple-cache": "^1.0.1",
        "yiisoft/router": "^3.0@dev"
    },
    "require-dev": {
        "nyholm/psr7": "^1.3",
        "phpunit/phpunit": "^9.4",
        "psr/container": "1.0",
        "roave/infection-static-analysis-plugin": "^1.4",
        "vimeo/psalm": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Router\\FastRoute\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Router\\FastRoute\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "common": "config/common.php",
            "web": "config/web.php"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "scripts": {
        "test": [
            "phpunit --colors --no-interaction"
        ]
    }
}
